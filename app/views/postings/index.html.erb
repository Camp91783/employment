<p id="notice"><%= notice %></p>

<h1>Listing Postings</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Company</th>
      <th>Location</th>
      <th>Contact</th>
      <th>Url</th>
      <th>Notes</th>
      <th>Date applied</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posting.each do |posting| %>
      <tr>
        <td><%= posting.title %></td>
        <td><%= posting.company %></td>
        <td><%= posting.location %></td>
        <td><%= posting.contact %></td>
        <td><%= posting.url %></td>
        <td><%= posting.notes %></td>
        <td><%= posting.date_applied %></td>
        <td><%= link_to 'Show', posting %></td>
        <td><%= link_to 'Edit', edit_posting_path(posting) %></td>
        <td><%= link_to 'Destroy', posting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Posting', new_posting_path %>

<p><a href="/">Back Home</a></p>

